# Private key for deployment (DO NOT commit the actual .env file)
# Export from MetaMask: Account Details > Show Private Key
PRIVATE_KEY=your_private_key_here

# Sepolia RPC URL (get from Alchemy or Infura)
SEPOLIA_RPC_URL=https://eth-sepolia.g.alchemy.com/v2/YOUR_API_KEY

# Etherscan API key for contract verification (get from etherscan.io)
ETHERSCAN_API_KEY=your_etherscan_api_key_here

